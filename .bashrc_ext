# Setting the color for the prompt
CYAN='\e[0;36m'
YELLOW='\e[1;33m'
ENDCOLOR="\e[0m"
export PS1="\[$CYAN\]> [\!] \u@\h :\[$YELLOW\] \W\[$CYAN\] $ \[$ENDCOLOR\]"

# Aliases
alias emacs="/Applications/MacPorts/Emacs.app/Contents/MacOS/Emacs"
alias ebashrc="emacs ~/.bashrc && source ~/.bashrc"
alias sbashrc="source ~/.bashrc"
